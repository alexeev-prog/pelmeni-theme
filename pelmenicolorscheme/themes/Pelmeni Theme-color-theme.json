{
  "name": "Pelmeni Theme",
  "colors": {
    "editor.background": "#202020",
    "editor.foreground": "#d9d9d9",
    "activityBarBadge.background": "#4C7C9A",
    "sideBarTitle.foreground": "#979796",
    "editorCursor.foreground": "#a6cded"
  },
  "editor.semanticTokenColorCustomizations": {
    "enabled": true,
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#B0B0B0"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#eeeeee"
      }
    },
    {
      "name": "String",
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#97d39a"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#ffafa5"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#de9bc8"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#85befd"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": [
        "meta.block variable.other"
      ],
      "settings": {
        "foreground": "#eeeeee"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": [
        "support.other.variable",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#94c9b2"
      }
    },
    {
      "name": "Function Arguments",
      "scope": [
        "variable.parameter",
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#f4bf75"
      }
    },
    {
      "name": "Class",
      "scope": [
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#ffdeaa"
      }
    },
    {
      "name": "Type",
      "scope": [
        "support.type"
      ],
      "settings": {
        "foreground": "#ffc591"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#ffafa5"
      }
    },
    {
      "name": "Interface",
      "scope": [
        "entity.name.interface"
      ],
      "settings": {
        "foreground": "#A0F1B6"
      }
    },
    {
      "name": "Array",
      "scope": [
        "meta.array",
        "variable.other.array"
      ],
      "settings": {
        "foreground": "#fd6853"
      }
    },
    {
      "name": "Property",
      "scope": [
        "variable.other.property",
        "meta.property"
      ],
      "settings": {
        "foreground": "#85befd"
      }
    },
    {
      "name": "Method",
      "scope": [
        "entity.name.method"
      ],
      "settings": {
        "foreground": "#85befd"
      }
    },
    {
      "name": "Decorator",
      "scope": [
        "meta.decorator",
        "entity.name.decorator"
      ],
      "settings": {
        "foreground": "#de9bc8"
      }
    },
    {

      "name": "Built-in Functions",
      "scope": [
        "support.function.builtin"
      ],
      "settings": {
        "foreground": "#85befd"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant"
      ],
      "settings": {
        "foreground": "#ffdeaa"
      }
    },
    {
      "name": "Namespace (C/C++)",
      "scope": [
        "entity.name.namespace"
      ],
      "settings": {
        "foreground": "#6A91CB"
      }
    },
    {
      "name": "Macro (C/C++)",
      "scope": [
        "meta.preprocessor"
      ],
      "settings": {
        "foreground": "#FF7F50"
      }
    },
    {
      "name": "C/C++ Variables",
      "scope": [
        "variable.other.c, variable.other.cpp"
      ],
      "settings": {
        "foreground": "#eb6f92"
      }
    },
    {
      "name": "C/C++ Function Call",
      "scope": [
        "meta.function-call.c, meta.function-call.cpp"
      ],
      "settings": {
        "foreground": "#85befd"
      }
    },
    {
      "name": "C/C++ Struct",
      "scope": [
        "entity.name.struct"
      ],
      "settings": {
        "foreground": "#f4bf75"
      }
    },
    {
      "name": "C/C++ Struct Elements (Namespaces)",
      "scope": [
        "variable.other.struct, variable.other.namespace"
      ],
      "settings": {
        "foreground": "#A0F1B6"
      }
    },
    {
      "name": "C/C++ Enum",
      "scope": [
        "entity.name.enum"
      ],
      "settings": {
        "foreground": "#ffdeaa"
      }
    },
    {
      "name": "C/C++ Template",
      "scope": [
        "entity.name.template"
      ],
      "settings": {
        "foreground": "#85befd"
      }
    }
  ]
}
